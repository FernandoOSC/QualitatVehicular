
@{
    ViewBag.Title = "RegistroPoliza";
}

<div class="top-content">
    <div class="container">

        <div class="row">
            <div class="col-sm-12 col-sm-offset-1 col-md-10 col-md-offset-2 col-lg-10 col-lg-offset-3 form-box">
                <form role="form" action="" method="post" class="f1">

                    <h3>Registro de Poliza</h3>
                    <div class="f1-steps">
                        <div class="f1-progress">
                            <div class="f1-progress-line" data-now-value="16.66" data-number-of-steps="3" style="width: 16.66%;"></div>
                        </div>
                        <div class="f1-step active">
                            <div class="f1-step-icon"><i class="fas fa-car"></i></div>
                            <p>Información del solicitante y vehículo</p>
                        </div>
                        <div class="f1-step">
                            <div class="f1-step-icon"><i class="fas fa-user"></i></div>
                            <p>Datos del contratante / contacto</p>
                        </div>
                        <div class="f1-step">
                            <div class="f1-step-icon"><i class="fas fa-paste"></i></div>
                            <p>Datos de la emisión</p>
                        </div>
                    </div>

                    <fieldset>
                        <h4>Datos del Vehículo:</h4>
                        <div id="contenedor_errorMessage" class="alert alert-danger Ocultar" role="alert">
                            <span id="errorMessage">simple danger alert—check it out!</span> 
                        </div>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Tipo de Vehículo:</label>
                                    <select class="form-control" id="sp_TipoVehiculo" name="sp_TipoVehiculo">
                                        <option value="">Automóvil</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Placa:</label>
                                    <input type="text"  class="form-control" id="placa_reg">
                                </div>
                                <div class="form-group">
                                    <label>Nro. Motor:</label>
                                    <input type="text" class="form-control" id="nromotor_reg">
                                </div>
                                <div class="form-group">
                                    <label>Nro. Serie:</label>
                                    <input type="text" class="form-control" id="nroserie_reg">
                                </div>                                


                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Marca:</label>
                                    <select class="form-control" id="sp_MarcaVehiculo" name="sp_MarcaVehiculo">
                                        <option value="">Toyota</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Nro. Asientos:</label>
                                    <input type="text" class="form-control" id="nroasientos_reg">
                                </div>

                                <div class="form-group">
                                    <label>Suma Asegurada:</label>
                                    <input type="text" class="form-control" id="sumaasegurada_reg">
                                </div>

                                <div class="form-group">
                                    <label>Tipo Uso:</label>
                                    <select class="form-control" id="sp_TipoUso" name="sp_TipoUso">
                                        <option value="">PARTICULAR</option>
                                    </select>
                                </div>


                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Modelo:</label>
                                    <select class="form-control" id="sp_ModeloVehiculo" name="sp_ModeloVehiculo">
                                        <option value="">Yaris</option>
                                    </select>
                                </div>

                                <div class="form-group">
                                    <label>Año de Fabricación:</label>
                                    <select class="form-control" id="sp_anioFabricacion">
                                        <option value="2007">2007</option>
                                        <option value="2008">2008</option>
                                        <option value="2009">2009</option>
                                        <option value="2010">2010</option>
                                    </select>

                                </div>
                                <div class="form-group">
                                    <label>Color:</label>
                                    <input type="text" class="form-control" id="color_reg">
                                </div>
                                <div class="form-group">
                                    <label>VIN:</label>
                                    <input type="text" class="form-control" id="vin_reg">
                                </div>


                            </div>
                        </div>

                        <div class="f1-buttons">
                            <button type="button" class="btn btn-next">Siguiente</button>
                        </div>

                    </fieldset>

                    <fieldset>
                        <h4>Datos del contratante / contacto:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Tipo de Documento</label>
                                    <select class="form-control" id="sp_TipoDocumento" name="sp_TipoDocumento">
                                        <option value="">Dni</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Nombres</label>
                                    <input type="text" class="form-control" id="nombres_reg">
                                </div>
                                <div class="form-group">
                                    <label>Fecha Nacimiento:</label>
                                    <input type="text" class="form-control" id="fecnaci_reg">
                                </div>
                                <div class="form-group">
                                    <label>Teléfono Fijo:</label>
                                    <input type="text" class="form-control" id="telefono_reg">
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Nro. Documento</label>
                                    <input type="text" class="form-control" id="nrodocumento_reg">
                                </div>
                                <div class="form-group">
                                    <label>Apellido Paterno</label>
                                    <input type="text" class="form-control" id="paterno_reg">
                                </div>
                                <div class="form-group">
                                    <label>Sexo:</label>
                                    <select class="form-control" id="sp_Sexo" name="sp_Sexo">
                                        <option value="">Masculino</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Celular</label>
                                    <input type="text" class="form-control" id="celular_reg">
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Nacionalidad:</label>
                                    <select class="form-control" id="sp_Nacionalidad" name="sp_Nacionalidad">
                                        <option value="">Peruana</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Apellido Materno</label>
                                    <input type="text" class="form-control" id="materno_reg">
                                </div>
                                <div class="form-group">
                                    <label>Estado Civil:</label>
                                    <select class="form-control" id="sp_EstadoCivil" name="sp_EstadoCivil">
                                        <option value="">Casado</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>E-mail:</label>
                                    <input type="text" class="form-control" id="email_reg">
                                </div>

                            </div>
                        </div>

                        <br>

                        <h4>Dirección:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Departamento:</label>
                                    <select class="form-control"  id="sp_Departamento" name="sp_Departamento">
                                        <option value="">LIMA</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Vía:</label>
                                    <select class="form-control" id="sp_TipoVia" name="sp_TipoVia">
                                        <option value="">CALLE</option>
                                    </select>
                                    <div style="height: 8px;"></div>
                                    <input type="text" class="form-control" placeholder="Dirección" id="direccion_reg">

                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Provincia:</label>
                                    <select class="form-control" id="sp_Provincia" name="sp_Provincia">
                                        <option value="">LIMA</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Número / Interior:</label>
                                    <input type="text" class="form-control" placeholder="" id="numeroubi_reg">
                                </div>


                            </div>
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="form-group">
                                    <label>Distrito:</label>
                                    <select class="form-control" id="sp_Distrito" name="sp_Distrito">
                                        <option value="">SAN ISIDRO</option>
                                    </select>
                                </div>



                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group">
                                    <label>Referencia:</label>
                                    <input type="text" class="form-control" placeholder="" id="referencia_reg">

                                </div>
                            </div>
                        </div>

                        <div class="f1-buttons">
                            <button type="button" class="btn btn-previous">Anterior</button>
                            <button type="button" class="btn btn-next">Siguiente</button>
                        </div>
                    </fieldset>

                    <fieldset>
                        <h4>Producto a Emitir:</h4>
                        <div class="row">

                            <div class="col-lg-4 col-md-4">
                                <div class="form-group">
                                    <label>Nro. Poliza:</label>
                                    <input type="text" class="form-control" id="nropoliza_reg">
                                </div>
                                <div class="form-group">
                                    <label>Tipo Poliza:</label>
                                    <select class="form-control" id="sp_Poliza" name="sp_Poliza">
                                        <option value="">NUEVO</option>

                                    </select>
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4">
                                <div class="form-group">
                                    <label>Vigencia(Inicio):</label>
                                    <input type="text" class="form-control" id="vigenciaini_reg">
                                </div>
                                <div class="form-group">
                                    <label>Plan / Producto:</label>
                                    <select class="form-control" id="sp_Plan">
                                        <option value="1">PLATINUM</option>
                                    </select>
                                </div>

                            </div>
                            <div class="col-lg-4 col-md-4">
                                <div class="form-group">
                                    <label>Vigencia(Fin):</label>
                                    <input type="text" class="form-control" id="vigenciafin_reg">
                                </div>
                            </div>
                        </div>
                        <br>


                        <div class="f1-buttons">
                            <button type="button" class="btn btn-previous">Anterior</button>
                            <button on type="button" class="btn btn-next" onclick="RegistrarPoliza_onclick()">Guardar</button>
                        </div>

                    </fieldset>

                </form>
            </div>
        </div>

    </div>
</div>




<!--Init Scripts-->    
<script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
<script type="text/javascript" src="~/Scripts/Genesis/Poliza/RegistroPoliza.js"></script>
<!--End Scripts-->

